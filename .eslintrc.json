{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "linebreak-style": "off",
    "spaced-comment": "off",
    "object-curly-newline": "off"
  }
}
